{
  "name": "wheel-es6",
  "version": "0.0.0",
  "description": "Es6 Starter",
  "main": "index.js",
  "scripts": {
    "dev": "npm run html:dev & npm run css:dev & npm run js:dev & npm run server:dev",
    "prod": "npm run html:prod & npm run css:prod & npm run js:prod",
    "server:dev": "browser-sync start --server html --files \"html/*.html, html/**/*.css, html/**/*.js\"",
    "html:dev": "jade src/index.jade -o html --doctype html5 --pretty --watch",
    "html:prod": "jade src/index.jade -o html --doctype html5",
    "css:dev": "stylus --use autoprefixer-stylus --use ./node_modules/stylus-font-face --with {limit:80000} --watch src/css/main.styl -o html/build/ --sourcemap",
    "css:prod": "stylus --use autoprefixer-stylus --use ./node_modules/stylus-font-face --with {limit:80000} src/css/main.styl -o html/build/ --compress",
    "js:dev": "watchify src/js/main.js -d -v -t babelify -t aliasify -o html/build/bundle.js",
    "js:prod": "browserify src/js/main.js -t babelify -t aliasify -t uglifyify -o html/build/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guins/wheel-es6.git"
  },
  "author": {
    "name": "Stéphane Guigné",
    "email": "stephane.guigne@gmail.com",
    "url": "http://stephaneguigne.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guins/wheel-es6/issues"
  },
  "homepage": "https://github.com/guins/wheel-es6#readme",
  "devDependencies": {
    "aliasify": "1.8.0",
    "autoprefixer-stylus": "0.8.1",
    "babel": "6.1.18",
    "babel-core": "6.2.1",
    "babel-eslint": "4.1.6",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-stage-0": "6.1.18",
    "babelify": "7.2.0",
    "browser-sync": "2.10.0",
    "browserify": "12.0.1",
    "jade": "1.11.0",
    "stylus": "0.52.4",
    "stylus-font-face": "1.1.0",
    "uglifyify": "3.0.1",
    "watchify": "3.6.1"
  }
}
